  "dataFlow": {
    "ragQuery": {
      "input": "User question (string)",
      "steps": [
        "1. Client sends question to /api/chat/query",
        "2. Backend generates embedding (Gemini text-embedding-004)",
        "3. PostgreSQL cosine similarity search in document_chunks",
        "4. Retrieve top-k chunks (k=5 default)",
        "5. Construct prompt with context + question",
        "6. Gemini generates answer with citations",
        "7. Return response to client"
      ],
      "output": "Answer (string) + Citations (array)"
    },
    "documentUpload": {
      "input": "PDF/Image file",
      "steps": [
        "1. Client uploads file via Uppy to /api/documents/upload",
        "2. Backend stores file in GCS",
        "3. OCR extraction (Tesseract + Gemini Vision)",
        "4. Document classification (Gemini)",
        "5. Quality assessment (Gemini)",
        "6. Semantic chunking (sentence-based)",
        "7. Embedding generation (Gemini text-embedding-004)",
        "8. Store in PostgreSQL (documents + document_chunks)",
        "9. Return document metadata to client"
      ],
      "output": "Document record + Processing status"
    },
    "benefitCalculation": {
      "input": "Household situation (JSON)",
      "steps": [
        "1. Client sends household data to /api/policyengine/calculate",
        "2. Backend spawns Python subprocess",
        "3. Python PolicyEngine calculates benefits",
        "4. Return benefit amounts per program",
        "5. (BLOCKED: libstdc++.so.6 dependency issue)"
      ],
      "output": "Benefit amounts (object) per program",
      "status": "Currently non-functional due to Python library dependency"
    },
    "eeExport": {
      "input": "Export request (navigator/caseworker)",
      "steps": [
        "1. Admin selects sessions to export",
        "2. Backend queries client_interaction_sessions",
        "3. Generate export file (CSV/JSON/XML)",
        "4. Create ee_export_batches record",
        "5. Store file in GCS",
        "6. Mark sessions as exported_to_ee=true",
        "7. Return download link"
      ],
      "output": "Export file (CSV/JSON/XML) + Batch record"
    }
  },
  
