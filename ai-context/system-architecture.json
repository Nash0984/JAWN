{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "systemName": "Maryland Multi-Program Benefits Navigator",
  "version": "1.0.0",
  "description": "AI-powered platform assisting users in understanding government benefit policies across 7 major Maryland programs using RAG + Rules as Code technology",
  "lastUpdated": "2025-01-11",
  
  "architectureType": "full-stack-monolith",
  "deploymentModel": "replit-cloud",
  
  "technologyStack": {
    "runtime": {
      "backend": "Node.js 20",
      "frontend": "Browser (ES2020+)"
    },
    "languages": {
      "primary": "TypeScript",
      "secondary": ["Python 3.11 (PolicyEngine)"]
    },
    "frameworks": {
      "backend": {
        "name": "Express.js",
        "version": "^4.18.0",
        "description": "RESTful API server with middleware support"
      },
      "frontend": {
        "name": "React",
        "version": "^18.2.0",
        "description": "Component-based UI framework",
        "buildTool": "Vite 5.x"
      },
      "routing": {
        "name": "Wouter",
        "version": "^3.0.0",
        "description": "Client-side routing"
      }
    },
    "database": {
      "type": "PostgreSQL",
      "provider": "Neon Database",
      "orm": "Drizzle ORM",
      "migrations": "Drizzle Kit (push-based, no manual SQL)"
    },
    "ai": {
      "provider": "Google Gemini",
      "models": {
        "text": "gemini-2.5-flash",
        "embeddings": "text-embedding-004",
        "vision": "gemini-2.5-flash (multimodal)"
      },
      "capabilities": ["RAG", "Document Analysis", "Chat", "Rules Extraction", "Semantic Search"]
    },
    "storage": {
      "files": "Google Cloud Storage",
      "sessions": "PostgreSQL (connect-pg-simple)",
      "vectors": "PostgreSQL (embedded in document_chunks table)"
    },
    "uiLibrary": {
      "name": "shadcn/ui",
      "base": "Radix UI primitives",
      "styling": "Tailwind CSS",
      "icons": ["lucide-react", "react-icons/si"]
    }
  },
  
  "architectureLayers": {
    "frontend": {
      "location": "client/src/",
      "responsibilities": [
        "User interface rendering",
        "Client-side routing (Wouter)",
        "State management (TanStack Query)",
        "Form validation (react-hook-form + Zod)",
        "Real-time updates (WebSocket client)"
      ],
      "keyDirectories": {
        "pages": "client/src/pages/ - Route components",
        "components": "client/src/components/ - Reusable UI components",
        "lib": "client/src/lib/ - Utilities and API client",
        "hooks": "client/src/hooks/ - Custom React hooks"
      },
      "stateManagement": {
        "server": "TanStack Query (React Query v5)",
        "local": "React useState/useReducer",
        "forms": "react-hook-form with zodResolver"
      }
    },
    "backend": {
      "location": "server/",
      "responsibilities": [
        "RESTful API endpoints",
        "Authentication (Passport.js LocalStrategy)",
        "Session management (PostgreSQL sessions)",
        "Business logic orchestration",
        "AI service integration",
        "Document processing pipeline",
        "PolicyEngine integration"
      ],
      "keyFiles": {
        "routes": "server/routes.ts - API endpoint definitions",
        "storage": "server/storage.ts - Data access layer (IStorage interface)",
        "auth": "server/auth.ts - Passport authentication setup",
        "vite": "server/vite.ts - Vite dev server integration"
      },
      "services": {
        "gemini": "server/services/gemini.service.ts - Google Gemini API integration",
        "email": "server/services/email.service.ts - Email notifications (inline templates)",
        "policyEngine": "server/services/policyengine.service.ts - Python subprocess wrapper",
        "notifications": "server/services/notification.service.ts - Real-time notifications",
        "websocket": "server/services/websocket.service.ts - WebSocket server"
      }
    },
    "database": {
      "location": "shared/schema.ts",
      "orm": "Drizzle ORM",
      "tables": 57,
      "keySchemas": [
        "users - User accounts with role-based access",
        "documents - Policy documents with metadata",
        "document_chunks - Chunked documents with embeddings (vector storage)",
        "rules - Rules as Code (structured policy rules)",
        "client_interaction_sessions - Navigator workspace sessions",
        "ee_export_batches - E&E export tracking",
        "notifications - In-app notification system",
        "policy_change_diffs - Version control for policy documents",
        "compliance_rules - Regulatory compliance validation",
        "intake_sessions - Adaptive intake copilot sessions",
        "screening_sessions - Anonymous benefit screener",
        "scenario_workspaces - Household scenario modeling",
        "evaluation_test_cases - Accuracy testing framework"
      ],
      "relationships": "Foreign keys with CASCADE delete, indexed queries",
      "vectorStorage": "Embeddings stored as REAL[] in document_chunks.embedding column"
    },
    "ai": {
      "provider": "Google Gemini API",
      "integration": "@google/genai SDK",
      "workflows": {
        "rag": {
          "description": "Retrieval-Augmented Generation for policy queries",
          "steps": [
            "1. Client sends question to /api/chat/query",
            "2. Backend generates embedding (Gemini text-embedding-004)",
            "3. PostgreSQL cosine similarity search in document_chunks",
            "4. Retrieve top-k relevant chunks",
            "5. Construct prompt with context + question",
            "6. Gemini generates answer with citations",
            "7. Return response to client"
          ],
          "endpoint": "/api/chat/query"
        },
        "documentProcessing": {
          "description": "Multi-stage document ingestion pipeline",
          "steps": [
            "1. Client uploads file via Uppy to /api/documents/upload",
            "2. Backend stores file in GCS",
            "3. OCR extraction (Tesseract + Gemini Vision)",
            "4. Document classification (Gemini)",
            "5. Quality assessment (Gemini)",
            "6. Semantic chunking (sentence-based)",
            "7. Embedding generation (Gemini text-embedding-004)",
            "8. Store in PostgreSQL (documents + document_chunks)",
            "9. Return document metadata to client"
          ],
          "endpoint": "/api/documents/upload"
        },
        "rulesExtraction": {
          "description": "Extract structured Rules as Code from policy documents",
          "steps": [
            "1. Analyze document text (Gemini)",
            "2. Identify rules (eligibility, calculation, procedure)",
            "3. Extract metadata (effective dates, citations)",
            "4. Store in rules table",
            "5. Link to source document"
          ],
          "endpoint": "/api/admin/extract-rules"
        },
        "intakeCopilot": {
          "description": "Conversational intake assistant with structured data extraction",
          "steps": [
            "1. Multi-turn dialogue (Gemini)",
            "2. Extract structured fields from conversation",
            "3. Track session progress",
            "4. PolicyEngine benefit calculation",
            "5. Generate application form when complete"
          ],
          "endpoint": "/api/intake/*"
        },
        "complianceValidation": {
          "description": "AI-powered regulatory compliance checking",
          "steps": [
            "1. Load compliance rules (WCAG, LEP, federal regs)",
            "2. Analyze document/rule against requirements (Gemini)",
            "3. Identify violations with severity scoring",
            "4. Generate remediation recommendations",
            "5. Store validation results"
          ],
          "endpoint": "/api/admin/compliance/validate"
        }
      }
    },
    "policyEngine": {
      "description": "Python-based benefit calculation engine",
      "package": "policyengine-us",
      "integration": "Python subprocess via child_process.spawn",
      "status": "BLOCKED - libstdc++.so.6 dependency issue",
      "capabilities": {
        "federal": ["SNAP", "Medicaid", "EITC", "CTC", "SSI"],
        "state": ["Maryland SNAP", "Maryland Medicaid", "Maryland TANF"]
      },
      "inputFormat": "JSON household situation (income, expenses, composition)",
      "outputFormat": "JSON benefit amounts per program",
      "workaround": "PolicyEngine REST API (unauthenticated endpoint returns empty results, paid endpoint requires Client ID/Secret)"
    }
  },
  
